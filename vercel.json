{
  "buildCommand": "pip install -r requirements.txt && python3 -m mkdocs build",
  "outputDirectory": "site",
  "installCommand": "pip install -r requirements.txt",
  "cleanUrls": false,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}

